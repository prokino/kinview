{
  "name": "kinview-react",
  "version": "0.1.0",
  "private": true,
  "//homepage": "http://vulcan.cs.uga.edu/prokino/kinview",
  "///homepage": "https://prokino.github.io/kinview",
  "homepage": "https://uga-gta.netlify.app/",
  "author": "Saber Soleymani",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.39",
    "array-move": "^2.2.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.2",
    "lodash": "^4.17.19",
    "material-ui-popup-state": "^1.5.4",
    "mysql2": "^2.1.0",
    "react": "^16.11",
    "react-dom": "^16.11",
    "react-helmet": "^6.1.0",
    "react-scripts": "^3.3.0",
    "react-sortable-hoc": "^1.10.1",
    "react-treebeard": "^3.2.4",
    "sequelize": "^6.3.3"
  },
  "main": "public/electron.js",
  "proxy": "http://localhost:8085",
  "scripts": {
    "start": "react-scripts start HOST=0.0.0.0",
    "build": "react-scripts build",
    "start_gta": "env REACT_APP_NAME=gta react-scripts start HOST=0.0.0.0",
    "build_gta": "env REACT_APP_NAME=gta node ./helpers/create-env.js && npm run build",
    "start_kinview": "env REACT_APP_NAME=kinase react-scripts start HOST=0.0.0.0",
    "build_kinview": "env REACT_APP_NAME=kinase node ./helpers/create-env.js && npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "react-build": "react-scripts build",
    "release": "yarn react-build && electron-builder --publish=always",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.22",
    "@types/react": "^16.9.43",
    "concurrently": "^5.0.2",
    "electron": "^7.1.7",
    "electron-builder": "^22.8.1",
    "gh-pages": "^2.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "wait-on": "^3.3.0"
  },
  "build": {
    "appId": "edu.uga.kinview",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
